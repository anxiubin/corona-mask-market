{
	"functions": {
		"source": "functions"
	},
	"hosting": {
		"public": "build",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "/api/**",
				"function": "api"
			}
		],
		"headers": [
			{
				"source": "**",
				"headers": [
					{
						"key": "Access-Control-Allow-Origin",
						"value": "*"
					}
				]
			}
		]
	}
}
